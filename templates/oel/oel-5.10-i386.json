{
  "builders": [
    {
      "vm_name": "oel510-i386",
      "guest_os_type": "RedHat",
      "type": "virtualbox-iso",
      "headless": true,
      "http_directory": "http",
      "iso_url": "http://mirrors.dotsrc.org/oracle-linux/EL5/U10/i386/Enterprise-R5-U10-Server-i386-dvd.iso",
      "iso_checksum": "5a9216dd69f17b9dd8f64ed539629741",
      "iso_checksum_type": "md5",
      "ssh_username": "vagrant",
      "ssh_password": "vagrant",
      "guest_additions_path": "VBoxGuestAdditions_{{.Version}}.iso",
      "virtualbox_version_file": ".vbox_version",
      "ssh_wait_timeout": "10000s",
      "boot_command": [
        "<tab> text ks=http://{{ .HTTPIP }}:{{ .HTTPPort}}/ks5.cfg<enter>"
      ],
      "disk_size": 8192,
      "vboxmanage": [
        ["modifyvm", "{{.Name}}", "--memory", "512"],
        ["modifyvm", "{{.Name}}", "--cpus", "1"]
      ],
      "shutdown_command": "echo 'vagrant'|sudo -S /sbin/shutdown -h now"
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "execute_command": "echo 'vagrant' | {{.Vars}} sudo -E -S bash '{{.Path}}'",
      "scripts": [
        "scripts/kernel.sh",
        "scripts/fix-slow-dns.sh",
        "scripts/sshd.sh",
        "scripts/vagrant.sh",
        "scripts/vbox.sh",
        "scripts/cleanup.sh",
        "scripts/zerodisk.sh"
      ]
    }
  ],
  "post-processors": [
    {
      "type": "vagrant",
      "output": "../../boxes/oel-5.10-i386-plain.box"
    }
  ]
}
